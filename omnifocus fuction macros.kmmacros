from datetime import datetime
import sys, os

def swapDate(strDate):
     d = datetime.strptime(strDate, '%A, %B %d, %Y %I:%M:%S %p')
     day_string = d.strftime('%Y-%m-%d_%H%M%S')
     return day_string

strDate = swapDate(os.getenv('KMVAR_OFTaskDate'))
